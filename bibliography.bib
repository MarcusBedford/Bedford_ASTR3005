	date-added = {2018-11-27 09:05:29 +0100},
	date-modified = {2018-11-27 09:05:29 +0100},
	doi = {10.1146/annurev-astro-082708-101650},
	eprint = {0904.4505},
	journal = {\araa},
	month = sep,
	pages = {371-425},
	primaryclass = {astro-ph.CO},
	title = {{Star-Formation Histories, Abundances, and Kinematics of Dwarf Galaxies in the Local Group}},
	volume = 47,
